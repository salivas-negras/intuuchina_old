                <field name="show_category_label" type="radio" class="btn-group" default="1" label='COM_JOBGROKLISTPOSTINGS_MODULES_MOD_JOBGROKPOSTINGS_MOD_JOBGROKPOSTINGS_XML_CATEGORY_LABEL' description='COM_JOBGROKLISTPOSTINGS_MODULES_MOD_JOBGROKPOSTINGS_MOD_JOBGROKPOSTINGS_XML_SHOWHIDE_CATEGORY'>
